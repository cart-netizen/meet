{
  "name": "meetup-local",
  "version": "1.0.0",
  "description": "MeetUp.local - Platform for local meetups in Russia",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "supabase gen types typescript --local > src/types/database.types.ts",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/datetimepicker": "^7.6.1",
    "@react-navigation/native": "^7.1.26",
    "@supabase/supabase-js": "^2.39.3",
    "date-fns": "^3.3.1",
    "date-fns-tz": "^3.0.0",
    "expo": "^54.0.31",
    "expo-constants": "~17.0.0",
    "expo-device": "~7.1.0",
    "expo-font": "~13.1.0",
    "expo-linking": "~7.0.5",
    "expo-location": "~18.1.0",
    "expo-notifications": "^0.31.4",
    "expo-router": "~4.0.0",
    "expo-secure-store": "~14.1.0",
    "expo-splash-screen": "^0.30.10",
    "expo-status-bar": "~2.1.0",
    "expo-web-browser": "~14.1.0",
    "immer": "^10.0.3",
    "lodash": "^4.17.21",
    "nanoid": "^5.0.4",
    "nativewind": "^2.0.11",
    "react": "^19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.50.0",
    "react-native": "^0.81.1",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "~3.17.0",
    "react-native-safe-area-context": "~5.0.0",
    "react-native-screens": "~4.5.0",
    "react-native-svg": "~15.11.0",
    "react-native-web": "~0.21.0",
    "zod": "^3.22.4",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.202",
    "@types/react": "~19.1.0",
    "@types/react-native": "^0.73.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "^54.0.9",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.4",
    "react-test-renderer": "^19.1.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "private": true
}
